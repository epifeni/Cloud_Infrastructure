{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "globaleducationdata"
		},
		"AzureBlobStorage_PrimarySchool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_PrimarySchool'"
		},
		"primary_enrollment_selected_countries_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ourworldindata.org/grapher/primary-enrollment-selected-countries.csv?v=1&csvType=full&useColumnShortNames=true"
		},
		"ls_st_globaleducationdata_stage_yt_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://globaleducationdata.blob.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "globaleducationdata_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_PrimarySchool_connectionString": {
						"value": "[parameters('AzureBlobStorage_PrimarySchool_connectionString')]"
					},
					"primary_enrollment_selected_countries_properties_typeProperties_url": {
						"value": "[parameters('primary_enrollment_selected_countries_properties_typeProperties_url')]"
					},
					"ls_st_globaleducationdata_stage_yt_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ls_st_globaleducationdata_stage_yt_properties_typeProperties_serviceEndpoint')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}